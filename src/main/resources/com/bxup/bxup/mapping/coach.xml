<?xml version="1.0" encoding="UTF-8"?>    
 <!DOCTYPE sqlMap    
 PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN"    
 "http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="fouthSpace">
	<typeAlias alias="Coach" type="com.bxup.bxup.model.Coach" />
	<!-- select -->
	<select id="selectAllCoach" resultClass="Coach">    
             select * from 
             t_coach where delete_status=0 ORDER BY create_time DESC
	</select>
	<!-- insert -->
	<insert id="insertCoachInfoForm" parameterClass="Coach">
		insert into
		t_coach(avatar,gym_id,name,create_time,approved,delete_status,gendar,age,genius,honor,profile,user_id,tab)
		values(#avatar#,#gym_id#,#name#,#create_time#,#approved#,#delete_status#,#gendar#,#age#,#genius#,#honor#,#profile#,#user_id#,#tab#)
	</insert>
	<!-- delete -->
	<delete id="deleteCoachInfoForm" parameterClass="Coach">
	</delete>
</sqlMap> 