<?xml version="1.0" encoding="UTF-8"?>    
 <!DOCTYPE sqlMap    
 PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN"    
 "http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="fouthSpace">
	<typeAlias alias="CoachInfoForm" type="com.wang.form.CoachInfoForm" />
	<!-- select -->
	<select id="selectAllCoach" resultClass="CoachInfoForm">    
             select * from 
             t_coach where delete_status=0 ORDER BY create_time DESC
	</select>
	<!-- insert -->
	<insert id="insertCoachInfoForm" parameterClass="CoachInfoForm">
		insert into
		t_coach(avatar,gym_id,name,create_time,approved,delete_status,gendar,age,genius,honor,profile,user_id)
		values(#avatar#,#gym_id#,#name#,#create_time#,#approved#,#delete_status#,#gendar#,#age#,#genius#,#honor#,#profile#,#user_id#)
	</insert>
	<!-- delete -->
	<delete id="deleteCoachInfoForm" parameterClass="java.lang.Integer">
		delete from t_event where create_date = #createDate#
	</delete>
</sqlMap> 