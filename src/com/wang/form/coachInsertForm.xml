<?xml version="1.0" encoding="UTF-8"?>    
 <!DOCTYPE sqlMap    
 PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN"    
 "http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="secondSpace">
	<!-- select -->
	<select id="getCoachInsertForm" parameterClass="java.lang.Integer"
		resultClass="com.wang.form.CoachForm">  
         <![CDATA[ 
             select imgtype from t_event where id = #id# 
         ]]>
	</select>
	<!-- insert -->
	<insert id="insertCoachInsertForm1" parameterClass="com.wang.form.CoachForm">
		insert into
		t_coach(avatar,gym_id,name,contact,create_time,approved,delete_status)
		values(#avatar#,#gym_id#,#name#,#contact#,#create_time#,#approved#,#delete_status#)
	</insert>
	<!-- delete -->
	<delete id="deleteCoachInsertForm" parameterClass="java.lang.Integer">
		delete from t_event where create_date = #createDate#
	</delete>
</sqlMap> 
